{
  "api": {
    "xeggex": {
      "baseUrl": "https://api.xeggex.com/api/v2",
      "rateLimit": {
        "requests": 100,
        "window": 60000
      }
    }
  },
  "trading": {
    "pairs": ["RVN"],
    "dataRetention": 1440,
    "updateInterval": 60000,
    "saveInterval": 300000,
    "enablePersistence": true
  },
  "server": {
    "port": 3000,
    "updateInterval": 2000
  },
  "logging": {
    "level": "info",
    "file": "logs/app.log",
    "storage": {
      "enabled": true,
      "level": "info",
      "debugOperations": false
    }
  },
  "storage": {
    "enabled": true,
    "cleanupMaxAge": 168,
    "autoCleanup": true
  }
}